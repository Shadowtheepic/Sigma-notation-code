#include <iostream>
#include<math.h>
using namespace std;

int main() {
    double sum = 0;
    int sum2 = 0;

    for(int i = -2; i <= 4; i++){
        sum += pow(2, 3 - i);
    }
    cout << "sum is " << sum << endl;

    for(int i = 0; i <= 8; i++){
        sum2 += cos(i * 3.14159265358979);
    }
    cout << "sum is " << sum2 << endl;

}
